{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType":"module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "semi": 0,
    "@typescript-eslint/no-explicit-any": 2,
    "space-before-function-paren": "off"
  }
}