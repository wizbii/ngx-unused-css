{
  "name": "@wizbii/ngx-unused-css",
  "version": "3.0.4",
  "description": "Detect unused CSS in angular components",
  "main": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "dev": "tsc -w",
    "build": "tsc",
    "lint": "eslint --ext .ts src/**",
    "lint:fix": "eslint --fix --ext .ts src/**",
    "pretty": "prettier 'src/**/*.ts' --list-different",
    "pretty:fix": "prettier 'src/**/*.ts' --list-different --write",
    "test": "jest src/** --runInBand",
    "e2e": "jest e2e/test.ts",
    "e2e:manual": "cd e2e && npm test",
    "e2e:init": "node dist/index --init",
    "prepare": "husky install"
  },
  "keywords": [
    "angular",
    "unused",
    "css",
    "detect",
    "find"
  ],
  "author": "Wizbii",
  "license": "ISC",
  "dependencies": {
    "chalk": "4.0.0",
    "jsdom": "19.0.0",
    "meow": "9.0.0",
    "prompts": "2.4.2",
    "purgecss": "5.0.0",
    "sass": "1.63.6",
    "table": "6.8.0"
  },
  "bin": {
    "ngx-unused-css": "./dist/index.js"
  },
  "bugs": "https://github.com/wizbii/ngx-unused-css/issues",
  "homepage": "https://github.com/wizbii/ngx-unused-css",
  "repository": {
    "type": "git",
    "url": "https://github.com/wizbii/ngx-unused-css"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@types/jest": "^29",
    "@types/jsdom": "^16.2.14",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "18",
    "@types/prompts": "^2.0.14",
    "@types/table": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^5.2.0",
    "jest": "29",
    "mock-fs": "^4.14.0",
    "nodemon": "^2.0.15",
    "prettier": "^3.0.0",
    "ts-jest": "29",
    "ts-node": "^10.9.1",
    "tslib": "2.6.0",
    "type-fest": "^4.0.0",
    "typescript": "5.1.6"
  }
}
